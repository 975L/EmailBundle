{
    "name": "c975l/email-bundle",
    "type": "symfony-bundle",
    "description": "Bundle to manage sending emails and store in a database ",
    "keywords": ["symfony", "bundle"],
    "homepage": "https://github.com/975L/EmailBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Laurent Marquet",
            "email": "laurent.marquet@laposte.net",
            "homepage": "https://github.com/Laurent3170",
            "role": "Developer"
        },
        {
            "name": "975L Community",
            "homepage": "https://github.com/975L"
        }
    ],
    "require": {
        "php": "^7",
        "c975l/config-bundle": "*",
        "c975l/includelibrary-bundle": "*",
        "c975l/services-bundle": "*",
        "c975l/toolbar-bundle": "*",
        "doctrine/doctrine-bundle": "^1",
        "doctrine/doctrine-cache-bundle": "^1",
        "doctrine/orm": "^2",
        "knplabs/knp-paginator-bundle": "^2",
        "symfony/form": "^4.2",
        "symfony/security": "^4.2",
        "symfony/swiftmailer-bundle": "^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3@dev",
        "rector/rector": "^0.3",
        "symplify/easy-coding-standard": "^5.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "c975L\\EmailBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "support": {
        "email": "contact@975l.com",
        "source": "https://github.com/975L/EmailBundle",
        "issues": "https://github.com/975L/EmailBundle/issues"
    }
}
