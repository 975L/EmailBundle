{
    "name": "c975l/email-bundle",
    "type": "symfony-bundle",
    "description": "Bundle to manage sending emails and store in a database ",
    "keywords": ["symfony", "bundle"],
    "homepage": "https://github.com/975L/EmailBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Laurent Marquet",
            "email": "laurent.marquet@laposte.net",
            "homepage": "https://github.com/LaurentMarquet",
            "role": "Developer"
        },
        {
            "name": "975L Community",
            "homepage": "https://github.com/975L"
        }
    ],
    "require": {
        "php": ">=8.0",
        "c975l/config-bundle": "*",
        "c975l/site-bundle": "*",
        "c975l/toolbar-bundle": "*",
        "doctrine/doctrine-bundle": "*",
        "doctrine/orm": "*",
        "knplabs/knp-paginator-bundle": "*",
        "symfony/form": "*",
        "symfony/security-bundle": "*",
        "symfony/twig-bundle": "*",
        "symfony/mailer": "*",
        "twig/cssinliner-extra": "*",
        "twig/extra-bundle": "*",
        "twig/intl-extra": "*"
    },
    "require-dev": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "c975L\\EmailBundle\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "support": {
        "email": "contact@975l.com",
        "source": "https://github.com/975L/EmailBundle",
        "issues": "https://github.com/975L/EmailBundle/issues"
    }
}
